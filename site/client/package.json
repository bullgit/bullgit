{
  "name": "website",
  "version": "0.1.0",
  "description": "Bullgit website",
  "license": "MIT",
  "author": "Gregor Adams <greg@pixelass.com>",
  "scripts": {
    "babel": "babel src --out-dir lib --extensions '.ts,.tsx'",
    "babel:prod": "cross-env NODE_ENV=production babel src --out-dir lib --extensions '.ts,.tsx'",
    "prebuild": "cross-env NODE_ENV=production yarn clean && yarn babel:prod",
    "build": "cross-env NODE_ENV=production yarn webpack:prod",
    "postbuild": "yarn clean:lib",
    "clean": "yarn clean:public; yarn clean:lib",
    "clean:lib": "rimraf lib",
    "clean:public": "rimraf public",
    "watch": "yarn clean && yarn babel && yarn webpack:dev",
    "webpack:dev": "webpack-dev-server --config webpack.dev.js --hot",
    "webpack:prod": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "@bullgit/styled-components": "0.1.0",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/styled-components": "4.1.4",
    "mapbox-gl": "0.52.0",
    "mobx": "5.8.0",
    "mobx-react": "5.4.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-mapbox-gl": "4.0.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/cli": "7.2.3",
    "@bullgit/cli": "0.1.0",
    "@bullgit/config-babel": "0.1.0",
    "@bullgit/config-prettier": "0.1.0",
    "@bullgit/config-stylelint": "0.1.0",
    "@bullgit/config-tslint": "0.1.0",
    "@bullgit/config-typescript": "0.1.0",
    "@bullgit/config-webpack": "0.1.0",
    "@types/mkdirp": "0.5.2",
    "@types/object-assign": "4.0.30",
    "@types/pify": "3.0.2",
    "concurrently": "4.1.0",
    "cosmiconfig": "5.0.7",
    "fbjs": "1.0.0",
    "html-react-parser": "0.6.1",
    "mkdirp": "0.5.1",
    "object-assign": "4.1.1",
    "pify": "4.0.1",
    "rimraf": "2.6.3",
    "typescript": "3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "publishConfig": {
    "access": "private"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions",
    "last 2 edge versions",
    "last 2 ios versions"
  ],
  "prvate": true
}
